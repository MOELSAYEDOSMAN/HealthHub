        "Args": {
          "connectionString": "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=HealthHub;TrustServerCertificate=True;Trusted_Connection=True;MultipleActiveResultSets=true",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "../logs/webapi-.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter,Serilog"
        }
      }
    ],
